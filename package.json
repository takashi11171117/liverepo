{
  "name": "live_repo_front",
  "version": "1.0.0",
  "description": "front of live_repo",
  "author": "Saito Takashi <takashi11171117@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "TEST=unit yarn ava ./resources/front/test/specs/* --verbose",
    "test:e2e": "php artisan migrate:refresh --seed && nightwatch -c ./resources/front/test/e2e/globals.json",
    "test": "yarn test:unit & yarn test:e2e",
    "test:build": "concurrently \"babel-node server-test.js\" \"php artisan migrate:refresh --seed && php artisan serve\""
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.0.1",
    "@nuxtjs/auth": "^4.6.6",
    "@nuxtjs/axios": "^5.3.3",
    "@nuxtjs/bulma": "^1.2.1",
    "@nuxtjs/font-awesome": "^1.0.3",
    "animate.css": "^3.7.0",
    "buefy": "^0.7.1",
    "core-js": "2",
    "cross-env": "^5.2.0",
    "image-compressor.js": "^1.1.4",
    "node-sass": "^4.11.0",
    "nuxt": "^2.4.0",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "style-loader": "^0.23.1",
    "sweetalert2": "^7.28.11",
    "universal-cookie": "^4.0.0",
    "v-calendar": "^0.9.7",
    "vue-image-lightbox": "^6.2.0",
    "vue-lazyload": "^1.2.6",
    "vue-touch": "^2.0.0-beta.4",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "browser-env": "^3.2.5",
    "chromedriver": "^2.46.0",
    "concurrently": "^4.1.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jsdom": "^12.2.0",
    "nightwatch": "^1.0.19",
    "nock": "^10.0.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-vue": "^2.0.0",
    "sass-loader": "^7.1.0",
    "selenium-server": "^3.141.59",
    "sinon": "^7.3.1",
    "swagger": "^0.7.5"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./resources/front/test/helpers/setup.js"
    ],
    "snapshotDir": "./resources/front/test/snapshots"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
